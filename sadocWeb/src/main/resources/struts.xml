<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.custom.i18n.resources" value="demo" />
	<package name="sadocWeb" namespace="/" extends="struts-default">

		<default-action-ref name="index" />

		<action name="signIn" class="fr.univartois.ili.sadoc.actions.ManageSignIn"
			method="execute">
			<result type="success">content/profile/profile.jsp</result>
		</action>

	</package>
</struts>